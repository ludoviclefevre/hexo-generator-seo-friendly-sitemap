<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="sitemap.xsl"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc><%= encodeURI(config.url) %></loc>
        <changefreq>daily</changefreq>
        <priority>1</priority>
    </url>
<% data.items.forEach(function(item){ %>
    <url>
        <loc><%- encodeURI(item.permalink) %></loc>
        <lastmod><%= item.updated.toISOString() || item.date.toISOString() %></lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.6</priority>
<% item.photos.forEach(function(photo){ %>
        <image:image>
            <image:loc><%- encodeURI(url.resolve(config.url, photo)) %></image:loc>
            <image:caption><![CDATA[<%= config.url %>]]></image:caption>
        </image:image>
<% }) %>
    </url>
<% }) %>
</urlset>
<% include meta %>